{
    "name": "trinityrank/multilanguage",
    "description": "Add alternate multilanguage tags for same pages but on other language",
    "keywords": [
        "laravel",
        "nova",
        "trinityrank",
        "alternate",
        "multilanguage"
    ],
    "homepage": "https://github.com/trinity-rank/multilanguage",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "TrinityRank DevTeam",
            "email": "trinitydevbg@gmail.com",
            "role": "Team"
        }
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Trinityrank\\Multilanguage\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Trinityrank\\Multilanguage\\MultilanguageServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
